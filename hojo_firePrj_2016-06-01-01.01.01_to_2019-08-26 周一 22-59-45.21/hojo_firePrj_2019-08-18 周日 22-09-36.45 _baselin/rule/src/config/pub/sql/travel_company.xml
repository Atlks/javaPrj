<?xml version="1.0" encoding="UTF-8"?>
<qrys>

    <travel_company_registration_info>
        <sql><![CDATA[
		select
		 *
		from TRAVEL_COMPANY_REGISTRATION_INFO
		where 1=1 {1}
		
		]]></sql>
        <params>
            <ID>
                <index>1</index>
                <con>and ID= :?</con>
                <type>char</type>
            </ID>
			<COMPANY_NAME>
				<index>2</index>
				<con>and COMPANY_NAME like :?</con>
				<type>varchar</type>
			</COMPANY_NAME>            
        </params>
    </travel_company_registration_info>

</qrys>